<!DOCTYPE html>
<html>

<head>
  <!--auto compiled css & Js-->
  <script type="text/javascript" src="//code.jquery.com/jquery-1.9.1.js"></script>

  <script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
  <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" />
  <!-- JavaScript for adding
    slider for multiple images
    shown at once-->
  <script type="text/javascript">
    $(window).load(function () {
      $(".carousel .item").each(function () {
        var i = $(this).next();
        i.length || (i = $(this).siblings(":first")),
          i.children(":first-child").clone().appendTo($(this));

        for (var n = 0; n < 4; n++)
          (i = i.next()).length || (i = $(this).siblings(":first")),
            i.children(":first-child").clone().appendTo($(this));
      });
    });
  </script>
</head>

<body style="text-align: center;">
  <!-- container class for bootstrap card-->
  <div class="container">

    <h2 style="text-align: center; margin-bottom:2% ; margin-top: 5%;">Les différentes catégories</h2>
    <!-- bootstrap card with row name myCarousel as row 1-->
    <div class="carousel slide" id="myCarousel">
      <div class="carousel-inner">
        <div class="item active">
          <div class="col-xs-2">
            <a href="/produits/maison">
              <img src="https://cuisinebroder.com/wp-content/uploads/2022/10/Projet-Nebbiolo-3-Broder-V2-scaled.jpg"
                class="img-responsive" style="width: 150px; height: 150px; object-fit: contain;">
            </a>
            <h4>Maison et Cuisine</h4>
          </div>
        </div>
        <div class="item">
          <div class="col-xs-2">
            <a href="/produits/sport">
              <img src="https://img.freepik.com/photos-gratuite/outils-sport_53876-138077.jpg?w=2000"
                class="img-responsive" style="width: 150px; height: 150px; object-fit: contain;">

            </a>
            <h4>Sport</h4>
          </div>
        </div>
        <div class="item">
          <div class="col-xs-2">
            <a href="/produits/sante">
              <img
                src="https://thumbs.dreamstime.com/b/aromatherapy-station-thermale-traitement-de-beaut%C3%A9-et-fond-bien-%C3%AAtre-avec-l-huile-massage-fleurs-d-orchid%C3%A9e-serviettes-produits-142951172.jpg"
                class="img-responsive" style="width: 150px; height: 150px; object-fit: contain;">
            </a>
            <h4>Santé et beauté</h4>
          </div>
        </div>
        <div class="item">
          <div class="col-xs-2">
            <a href="/produits/electronique">
              <img
                src="data:image/jpeg;base64,"
                class="img-responsive" style="width: 150px; height: 150px; object-fit: contain;">
            </a>
            <h4>Electronique</h4>
          </div>
        </div>
        <div class="item">
          <div class="col-xs-2">
            <a href="/produits/mode">
              <img
                src="https://assets-global.website-files.com/631e047ded5805d36bbd2efd/63233456a972a7c3fd349b0b_63220d2b8120db3bd6705cff_a3f35407-c86e-4f91-be21-59c6d305aad0_magasin_vetement.png"
                class="img-responsive" style="width: 150px; height: 150px; object-fit: contain;">
            </a>
            <h4>Mode</h4>
          </div>
        </div>
      </div>


      <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <i class="glyphicon glyphicon-chevron-left"> </i>
      </a>
      <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <i class="glyphicon glyphicon-chevron-right"> </i>
      </a>
    </div>
  </div>

  <h4>
    <a href="/produits">
      Voir tous nos produits
      <i class="fa-solid fa-arrow-right fa-beat"></i>
    </a>
  </h4>
  <h2 style="text-align: center; margin-bottom: 20px; margin-top: 2%;">
    Les meilleures offres
  </h2>
  <div class="container">
    <div class="row">
      <% produits.forEach(function(produit, index) { if (index < 3) { %>
        <div class="col">
          <div class="card mb-5">
            <a href="/produits/detailsProduit/<%= produit._id %>">
              <img src="<%= produit.image%>" height="200" width="200"
              style="object-fit: contain; border-bottom: 1px solid gray" class="card-img-top" alt="..." />
            </a>
            <div class="card-body" style="text-align: center;">
              <h5 class="card-title"><strong>
                  <%= produit.nom %> - <%= produit.prix %>$ CAD
                </strong></h5>
              <a href="/produits/detailsProduit/<%= produit._id %>" class="btn btn-primary">Voir l'article</a>
            </div>
          </div>
        </div>
        <% } }); %>
    </div>

    <div class="row">
      <% produits.forEach(function(produit, index) { if (index>= 3 && index < 6) { %>
          <div class="col">
            <div class="card mb-5">
              <a href="/produits/detailsProduit/<%= produit._id %>">
                <img src="<%= produit.image%>" height="200" width="200"
                style="object-fit: contain; border-bottom: 1px solid gray" class="card-img-top" alt="..." />
              </a>
              <div class="card-body" style="text-align: center;">
                <h5 class="card-title"><strong>
                    <%= produit.nom %> - <%= produit.prix %>$ CAD
                  </strong></h5>
                  <a href="/produits/detailsProduit/<%= produit._id %>" class="btn btn-primary">Voir l'article</a>
              </div>
            </div>
          </div>
          <% } }); %>
    </div>
  </div>
</body>

</html>